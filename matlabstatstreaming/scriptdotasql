CREATE TABLE public.tn (
	id serial NOT NULL PRIMARY KEY,
	id_tn int NOT NULL unique,
	name_tn text NOT NULL,
	closed int4 NOT NULL,
	dateend date NOT NULL,
	withqualif int4 NOT NULL,
	qualifend date NULL,
	type varchar NULL,
	qualifclosed int4 NULL
);

CREATE TABLE public.patch (
	id serial NOT NULL,
	patchnum int4 NULL unique,
	patchind varchar NULL,
	patchdate date NULL,
	CONSTRAINT patch_pk PRIMARY KEY (id)
);

CREATE TABLE public.team (
	id serial not null primary key,
	team_id int4 NOT NULL unique,
	team_name varchar(255) NOT NULL
);

CREATE TABLE public.hero (
  id int PRIMARY KEY,
  name text,
  displayname text,
  primary_attr text,
  attack_type text
);

CREATE TABLE public.patch (
	id serial not null primary key,
	patch_id int4 NOT NULL unique,
	patch_number int NULL,
	patch_indice varchar(50) NULL,
	patchinit int2 NULL
);

CREATE TABLE public.matchs (
	id serial not null primary key,
	match_id bigint NOT NULL,
	patch_id int NOT NULL,
	id_tn int NOT NULL,
	series_id bigint NOT NULL,
	radiant_team_id bigint NOT NULL,
	dire_team_id bigint NOT NULL,
	start_time timestamp NOT NULL,
	CONSTRAINT matchs_fk FOREIGN KEY (radiant_team_id) REFERENCES team(team_id),
	CONSTRAINT matchs_fk_1 FOREIGN KEY (dire_team_id) REFERENCES team(team_id),
	CONSTRAINT matchs_fk_2 FOREIGN KEY (id_tn) REFERENCES tn(id_tn)
);

CREATE TABLE public.openmatch (
	id serial NOT NULL primary key,
	match_id bigint NOT NULL unique,
	id_tn int NOT NULL,
	radiant_team_id bigint NOT NULL,
	dire_team_id bigint NOT NULL,
	randiant_score int NOT NULL,
	dire_score int NOT NULL,
	duration int NOT NULL,
	firstpick int2 NOT NULL,
	radiant_win int2 NOT NULL,
	radiant_b1 int NULL,
	radiant_b2 int NULL,
	radiant_b3 int NULL,
	radiant_b4 int NULL,
	radiant_b5 int NULL,
	radiant_b6 int NULL,
	radiant_p1 int NULL,
	radiant_p2 int NULL,
	radiant_p3 int NULL,
	radiant_p4 int NULL,
	radiant_p5 int NULL,
	dire_b1 int NULL,
	dire_b2 int NULL,
	dire_b3 int NULL,
	dire_b4 int NULL,
	dire_b5 int NULL,
	dire_b6 int NULL,
	dire_p1 int NULL,
	dire_p2 int NULL,
	dire_p3 int NULL,
	dire_p4 int NULL,
	dire_p5 int NULL,
	comeback int NULL,
	stomp int NULL,
	CONSTRAINT openmatch_fk FOREIGN KEY (id_tn) REFERENCES public.tn(id_tn),
	CONSTRAINT openmatch_fk_1 FOREIGN KEY (radiant_team_id) REFERENCES public.team(team_id),
	CONSTRAINT openmatch_fk_2 FOREIGN KEY (dire_team_id) REFERENCES public.team(team_id),
	CONSTRAINT openmatch_fk_3 FOREIGN KEY (radiant_b1) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_4 FOREIGN KEY (radiant_b2) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_5 FOREIGN KEY (radiant_b3) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_6 FOREIGN KEY (radiant_b4) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_7 FOREIGN KEY (radiant_b5) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_8 FOREIGN KEY (radiant_b6) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_9 FOREIGN KEY (radiant_p1) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_10 FOREIGN KEY (radiant_p2) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_11 FOREIGN KEY (radiant_p3) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_12 FOREIGN KEY (radiant_p4) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_13 FOREIGN KEY (radiant_p5) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_14 FOREIGN KEY (dire_b1) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_15 FOREIGN KEY (dire_b2) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_16 FOREIGN KEY (dire_b3) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_17 FOREIGN KEY (dire_b4) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_18 FOREIGN KEY (dire_b5) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_19 FOREIGN KEY (dire_b6) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_20 FOREIGN KEY (dire_p1) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_21 FOREIGN KEY (dire_p2) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_22 FOREIGN KEY (dire_p3) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_23 FOREIGN KEY (dire_p4) REFERENCES public.hero(id),
	CONSTRAINT openmatch_fk_24 FOREIGN KEY (dire_p5) REFERENCES public.hero(id)
);

CREATE TABLE public.valvematch (
	id serial not null primary key,
	match_id bigint not NULL unique,
	id_tn int NOT NULL,
	radiant_name varchar(255) NULL,
	dire_name varchar(255) NULL,
	radiant_team_id bigint NOT NULL,
	dire_team_id bigint NOT NULL,
	randiant_score int NULL,
	dire_score int NULL,
	duration bigint NULL,
	firstpick int2 NULL,
	radiant_win int2 NULL,
	radiant_drafter int NULL,
	dire_drafter int NULL,
	radiant_b1 int NULL,
	radiant_b2 int NULL,
	radiant_b3 int NULL,
	radiant_b4 int NULL,
	radiant_b5 int NULL,
	radiant_b6 int NULL,
	radiant_p1 int NULL,
	radiant_p2 int NULL,
	radiant_p3 int NULL,
	radiant_p4 int NULL,
	radiant_p5 int NULL,
	dire_b1 int NULL,
	dire_b2 int NULL,
	dire_b3 int NULL,
	dire_b4 int NULL,
	dire_b5 int NULL,
	dire_b6 int NULL,
	dire_p1 int NULL,
	dire_p2 int NULL,
	dire_p3 int NULL,
	dire_p4 int NULL,
	dire_p5 int NULL,
	CONSTRAINT valvematch_fk FOREIGN KEY (id_tn) REFERENCES public.tn(id_tn),
	CONSTRAINT valvematch_fk_1 FOREIGN KEY (radiant_team_id) REFERENCES public.team(team_id),
	CONSTRAINT valvematch_fk_2 FOREIGN KEY (dire_team_id) REFERENCES public.team(team_id),
	CONSTRAINT valvematch_fk_3 FOREIGN KEY (radiant_b1) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_4 FOREIGN KEY (radiant_b2) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_5 FOREIGN KEY (radiant_b3) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_6 FOREIGN KEY (radiant_b4) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_7 FOREIGN KEY (radiant_b5) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_8 FOREIGN KEY (radiant_b6) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_9 FOREIGN KEY (radiant_p1) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_10 FOREIGN KEY (radiant_p2) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_11 FOREIGN KEY (radiant_p3) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_12 FOREIGN KEY (radiant_p4) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_13 FOREIGN KEY (radiant_p5) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_14 FOREIGN KEY (dire_b1) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_15 FOREIGN KEY (dire_b2) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_16 FOREIGN KEY (dire_b3) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_17 FOREIGN KEY (dire_b4) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_18 FOREIGN KEY (dire_b5) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_19 FOREIGN KEY (dire_b6) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_20 FOREIGN KEY (dire_p1) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_21 FOREIGN KEY (dire_p2) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_22 FOREIGN KEY (dire_p3) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_23 FOREIGN KEY (dire_p4) REFERENCES public.hero(id),
	CONSTRAINT valvematch_fk_24 FOREIGN KEY (dire_p5) REFERENCES public.hero(id)
);

CREATE TABLE public.valveplayermatch (
	id serial NOT NULL PRIMARY KEY,
	account_id bigint NOT NULL,
	match_id bigint NOT NULL,
	player_slot int NULL,
	hero_id int NOT NULL,
	item_0 int NULL,
	item_1 int NULL,
	item_2 int NULL,
	item_3 int NULL,
	item_4 int NULL,
	item_5 int NULL,
	backpack_0 int NULL,
	backpack_1 int NULL,
	backpack_2 int NULL,
	kills int NULL,
	deaths int NULL,
	assists int NULL,
	leaver_status int2 NULL,
	last_hits int NULL,
	denies int NULL,
	gold_per_min int NULL,
	xp_per_min int NULL,
	level int NULL,
	hero_damage int NULL,
	tower_damage int NULL,
	hero_healing int NULL,
	gold int NULL,
	gold_spent int NULL,
	scaled_hero_damage int NULL,
	scaled_tower_damage int NULL,
	scaled_hero_healing int NULL,
	unique (match_id,account_id),
	CONSTRAINT valveplayermatch_fk FOREIGN KEY (match_id) REFERENCES matchs(match_id),
	CONSTRAINT valveplayermatch_fk_1 FOREIGN KEY (hero_id) REFERENCES hero(id),
	CONSTRAINT valveplayermatch_fk_2 FOREIGN KEY (account_id) REFERENCES player(player_id),
	CONSTRAINT valveplayermatch_fk_3 FOREIGN KEY (item_0) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_4 FOREIGN KEY (item_1) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_5 FOREIGN KEY (item_2) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_6 FOREIGN KEY (item_3) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_7 FOREIGN KEY (item_4) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_8 FOREIGN KEY (item_5) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_9 FOREIGN KEY (backpack_0) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_10 FOREIGN KEY (backpack_1) REFERENCES public.item(id),
	CONSTRAINT valveplayermatch_fk_11 FOREIGN KEY (backpack_2) REFERENCES public.item(id)
);

CREATE TABLE public.player (
	id serial not null primary key,
	player_id int4 NOT NULL,
	player_name varchar(255) NULL,
	team_id int4 NULL
);

CREATE TABLE public.item (
	id int4 NOT NULL,
	name varchar(250) NULL,
	displayname varchar(250) NULL,
	shortname varchar(250) NULL,
	cost int4 NULL,
	secret_shop int2 NULL,
	side_shop int2 NULL,
	recipe int2 NULL,
	support int2 NULL,
	CONSTRAINT item_pkey PRIMARY KEY (id)
);

CREATE TABLE public.openplayermatch (
	id serial NOT NULL PRIMARY KEY,
	match_id bigint NOT NULL,
	account_id bigint NOT NULL,
	player_slot int NOT NULL,
	hero_id int NOT NULL,
	item_0 int NULL,
	item_1 int NULL,
	item_2 int NULL,
	item_3 int NULL,
	item_4 int NULL,
	item_5 int NULL,
	backpack_0 int NULL,
	backpack_1 int NULL,
	backpack_2 int NULL,
	obs_placed numeric(38) NULL,
	sen_placed numeric(38) NULL,
	kills numeric(38) NULL,
	deaths numeric(38) NULL,
	assists numeric(38) NULL,
	leaver_status numeric(38) NULL,
	camps_stacked numeric(38) NULL,
	creep_stacked numeric(38) NULL,
	last_hits numeric(38) NULL,
	denies numeric(38) NULL,
	gold_per_min numeric(38) NULL,
	xp_per_min numeric(38) NULL,
	"level" numeric(38) NULL,
	gold numeric(38) NULL,
	gold_spent numeric(38) NULL,
	firstblood_claimed numeric(38) NULL,
	roshans_kill numeric(38) NULL,
	tower_kill numeric(38) NULL,
	kda numeric(38) NULL,
	hero_damage numeric(38) NULL,
	tower_damage numeric(38) NULL,
	hero_healing numeric(38) NULL,
	rune_pickups numeric(38) NULL,
	dd_pickups numeric(38) NULL,
	haste_pickups numeric(38) NULL,
	regen_pickups numeric(38) NULL,
	invi_pickups numeric(38) NULL,
	illu_pickups numeric(38) NULL,
	bounty_pickups numeric(38) NULL,
	arcane_pickups numeric(38) NULL,
	stun numeric(38) NULL,
	teamfight_part numeric(38) NULL,
	lane_effi numeric(38) NULL,
	lane_effi_pct numeric(38) NULL,
	lane numeric(38) NULL,
	lane_role numeric(38) NULL,
	apm numeric(38) NULL,
	dead_time numeric(38) NULL,
	tango numeric(38) NULL,
	tpscroll numeric(38) NULL,
	arcane_boots numeric(38) NULL,
	clarity numeric(38) NULL,
	smoke_of_deceit numeric(38) NULL,
	quelling_blade numeric(38) NULL,
	phase_boots numeric(38) NULL,
	shadow_amulet numeric(38) NULL,
	invis_sword numeric(38) NULL,
	pipe numeric(38) NULL,
	soul_ring numeric(38) NULL,
	medallion_of_courage numeric(38) NULL,
	helm_of_the_dominator numeric(38) NULL,
	branches numeric(38) NULL,
	tome_of_knowledge numeric(38) NULL,
	diffusal_blade numeric(38) NULL,
	power_treads numeric(38) NULL,
	bottle numeric(38) NULL,
	glimmer_cape numeric(38) NULL,
	black_king_bar numeric(38) NULL,
	silver_edge numeric(38) NULL,
	blink numeric(38) NULL,
	blade_mail numeric(38) NULL,
	travel_boots numeric(38) NULL,
	urn_of_shadows numeric(38) NULL,
	guardian_greaves numeric(38) NULL,
	hurricane_pike numeric(38) NULL,
	cheese numeric(38) NULL,
	solar_crest numeric(38) NULL,
	mjollnir numeric(38) NULL,
	dust numeric(38) NULL,
	spirit_vessel numeric(38) NULL,
	cyclone numeric(38) NULL,
	manta numeric(38) NULL,
	refresher_shard numeric(38) NULL,
	moon_shard numeric(38) NULL,
	crimson_guard numeric(38) NULL,
	butterfly numeric(38) NULL,
	rod_of_atos numeric(38) NULL,
	orchid numeric(38) NULL,
	shivas_guard numeric(38) NULL,
	dagon numeric(38) NULL,
	veil_of_discord numeric(38) NULL,
	ethereal_blade numeric(38) NULL,
	ghost numeric(38) NULL,
	nullifier numeric(38) NULL,
	satanic numeric(38) NULL,
	sheepstick numeric(38) NULL,
	heavens_halberd numeric(38) NULL,
	mekansm numeric(38) NULL,
    unique (match_id,account_id),
	CONSTRAINT openplayermatch_fk FOREIGN KEY (match_id) REFERENCES matchs(match_id),
	CONSTRAINT openplayermatch_fk_1 FOREIGN KEY (hero_id) REFERENCES hero(id),
	CONSTRAINT openplayermatch_fk_2 FOREIGN KEY (account_id) REFERENCES player(player_id),
	CONSTRAINT openplayermatch_fk_3 FOREIGN KEY (item_0) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_4 FOREIGN KEY (item_1) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_5 FOREIGN KEY (item_2) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_6 FOREIGN KEY (item_3) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_7 FOREIGN KEY (item_4) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_8 FOREIGN KEY (item_5) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_9 FOREIGN KEY (backpack_0) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_10 FOREIGN KEY (backpack_1) REFERENCES public.item(id),
	CONSTRAINT openplayermatch_fk_11 FOREIGN KEY (backpack_2) REFERENCES public.item(id)
);

CREATE TABLE public.stat_global_hero (
	id serial NOT NULL,
	id int4 NOT NULL,
	nb_pick int4 NULL,
	nb_ban int4 NULL,
	nb_match int4 NULL,
	mean_is_win numeric NULL,
	max_is_win numeric NULL,
	min_is_win numeric NULL,
	tot_is_win numeric NULL,
	mean_kills numeric NULL,
	max_kills numeric NULL,
	min_kills numeric NULL,
	tot_kills numeric NULL,
	mean_assists numeric NULL,
	max_assists numeric NULL,
	min_assists numeric NULL,
	tot_assists numeric NULL,
	mean_deaths numeric NULL,
	max_deaths numeric NULL,
	min_deaths numeric NULL,
	tot_deaths numeric NULL,
	mean_last_hits numeric NULL,
	max_last_hits numeric NULL,
	min_last_hits numeric NULL,
	tot_last_hits numeric NULL,
	mean_denies numeric NULL,
	max_denies numeric NULL,
	min_denies numeric NULL,
	tot_denies numeric NULL,
	mean_gold_per_min numeric NULL,
	max_gold_per_min numeric NULL,
	min_gold_per_min numeric NULL,
	tot_gold_per_min numeric NULL,
	mean_xp_per_min numeric NULL,
	max_xp_per_min numeric NULL,
	min_xp_per_min numeric NULL,
	tot_xp_per_min numeric NULL,
	mean_hero_damage numeric NULL,
	max_hero_damage numeric NULL,
	min_hero_damage numeric NULL,
	tot_hero_damage numeric NULL,
	mean_tower_damage numeric NULL,
	max_tower_damage numeric NULL,
	min_tower_damage numeric NULL,
	tot_tower_damage numeric NULL,
	mean_level numeric NULL,
	max_level numeric NULL,
	min_level numeric NULL,
	tot_level numeric NULL,
	mean_hero_healing numeric NULL,
	max_hero_healing numeric NULL,
	min_hero_healing numeric NULL,
	tot_hero_healing numeric NULL,
	CONSTRAINT stat_global_hero_pkey PRIMARY KEY (id),
	CONSTRAINT stat_global_hero_fk FOREIGN KEY (id) REFERENCES hero(id)
);

