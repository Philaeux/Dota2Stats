	CONSTRAINT stat_patch_team_fk_1 FOREIGN KEY (team_id) REFERENCES team(team_id),
	CONSTRAINT stat_patch_team_fk_2 FOREIGN KEY (id) REFERENCES hero(id)
);

CREATE TABLE public.stat_patch_player_hero (
	id serial NOT NULL,
	patch_id int8 NOT NULL,
	id int NULL,
	id_tn int NULL,
	account_id bigint NULL,
	nb_pick numeric(38,6) NULL,
	nb_ban numeric(38,6) NULL,
	nb_match numeric(38,6) NULL,
