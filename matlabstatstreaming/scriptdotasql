CREATE TABLE public.stat_patch_team (
	id serial NOT NULL,
	patch_id int8 NOT NULL,
	team_id int4 NULL,
	mean_is_radiant numeric NULL,
	max_is_radiant numeric NULL,
	min_is_radiant numeric NULL,
	tot_is_radiant numeric NULL,
	mean_is_radiant_win numeric NULL,
	max_is_radiant_win numeric NULL,
	min_is_radiant_win numeric NULL,
	tot_is_radiant_win numeric NULL,
	mean_is_dire_win numeric NULL,
	max_is_dire_win numeric NULL,
	min_is_dire_win numeric NULL,
	tot_is_dire_win numeric NULL,
	mean_is_win numeric NULL,
	max_is_win numeric NULL,
	min_is_win numeric NULL,
	tot_is_win numeric NULL,
	mean_is_firstpick numeric NULL,
	max_is_firstpick numeric NULL,
	min_is_firstpick numeric NULL,
	tot_is_firstpick numeric NULL,
	mean_duration numeric NULL,
	max_duration numeric NULL,
	min_duration numeric NULL,
	tot_duration numeric NULL,
	nb_match numeric(38) NULL,
	CONSTRAINT stat_patch_team_pkey PRIMARY KEY (id),
	CONSTRAINT stat_patch_team_fk FOREIGN KEY (team_id) REFERENCES team(team_id)
	CONSTRAINT stat_patch_team_fk1 FOREIGN KEY (patch_id) REFERENCES patch(patch_id)
);

CREATE TABLE public.stat_patch_team_hero (
	id serial NOT NULL,
	patch_id int8 NOT NULL,
	id int NULL,
	id_tn int NULL,
	team_id int NULL,
	nb_pick numeric(38,6) NULL,
	nb_pick_against numeric(38,6) NULL,
	nb_ban numeric(38,6) NULL,
	nb_ban_against numeric(38,6) NULL,
	nb_match numeric(38,6) NULL,
	mean_is_win numeric(38,6) NULL,
	max_is_win numeric(38,6) NULL,
	min_is_win numeric(38,6) NULL,
	tot_is_win numeric(38,6) NULL,
	mean_kills numeric(38,6) NULL,
	max_kills numeric(38,6) NULL,
	min_kills numeric(38,6) NULL,
	tot_kills numeric(38,6) NULL,
	mean_assists numeric(38,6) NULL,
	max_assists numeric(38,6) NULL,
	min_assists numeric(38,6) NULL,
	tot_assists numeric(38,6) NULL,
	mean_deaths numeric(38,6) NULL,
	max_deaths numeric(38,6) NULL,
	min_deaths numeric(38,6) NULL,
	tot_deaths numeric(38,6) NULL,
	mean_last_hits numeric(38,6) NULL,
	max_last_hits numeric(38,6) NULL,
	min_last_hits numeric(38,6) NULL,
	tot_last_hits numeric(38,6) NULL,
	mean_denies numeric(38,6) NULL,
	max_denies numeric(38,6) NULL,
	min_denies numeric(38,6) NULL,
	tot_denies numeric(38,6) NULL,
	mean_gold_per_min numeric(38,6) NULL,
	max_gold_per_min numeric(38,6) NULL,
	min_gold_per_min numeric(38,6) NULL,
	tot_gold_per_min numeric(38,6) NULL,
	mean_xp_per_min numeric(38,6) NULL,
	max_xp_per_min numeric(38,6) NULL,
	min_xp_per_min numeric(38,6) NULL,
	tot_xp_per_min numeric(38,6) NULL,
	mean_hero_damage numeric(38,6) NULL,
	max_hero_damage numeric(38,6) NULL,
	min_hero_damage numeric(38,6) NULL,
	tot_hero_damage numeric(38,6) NULL,
	mean_tower_damage numeric(38,6) NULL,
	max_tower_damage numeric(38,6) NULL,
	min_tower_damage numeric(38,6) NULL,
	tot_tower_damage numeric(38,6) NULL,
	mean_level numeric(38,6) NULL,
	max_level numeric(38,6) NULL,
	min_level numeric(38,6) NULL,
	tot_level numeric(38,6) NULL,
	mean_hero_healing numeric(38,6) NULL,
